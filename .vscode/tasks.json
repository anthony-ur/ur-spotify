{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Serve dbt docs",
            "type": "shell",
            "command": "dbt docs generate && dbt docs serve"
        },
        {
            "label": "Recompile dbt",
            "type": "shell",
            "command": "dbt clean && dbt compile"
        },
        {
            "label": "Run dbt",
            "type": "shell",
            "command": "dbt run"
        }, 
    ]
}